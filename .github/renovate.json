{
  "assignees": ["alvr"],
  "baseBranches": ["develop"],
  "extends": ["config:base", ":disableDependencyDashboard"],
  "rebaseWhen": "behind-base-branch",
  "reviewers": ["alvr"],
  "packageRules": [
    {
      "groupName": "compose",
      "matchPackagePrefixes": ["androidx.compose"]
    },
    {
      "automerge": true,
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"]
    }
  ]
}
